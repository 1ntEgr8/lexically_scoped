<style>
    .resume section {
        padding-bottom: 0px;
    }

    .resume__header {
        display: flex;
        justify-content: space-between;
        align-items: center;
        flex-wrap: wrap;
    }

    .resume__container {
        display: flex;
        flex-direction: column;
        flex-wrap: wrap;
    }

    .resume__container__item {
        margin-bottom: 20px;
    }

    .resume__container__item__row {
        font-size: 1rem;
    }

    .resume__container__item__row--header {
        display: flex;
        justify-content: space-between;
        flex-wrap: wrap;
    }

    .resume__container__item__row ul {
        margin-bottom: 0px;
    }
</style>

